      <foaf:name>Kai Willadsen</foaf:name>
      <foaf:mbox rdf:resource="mailto:kai.willadsen@gmail.com" />
      <gnome:userid>kaiw</gnome:userid>
    </foaf:Person>
  </maintainer>

</Project>
